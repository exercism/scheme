chez := scheme

all : check-chez check-guile

check-chez : 
	echo "(when (eq? 'failure (test)) (error 'chez \"fail!\"))" | scheme -q example.scm

check-guile : 
	guile --no-auto-compile -q -l example.scm -c \
		"(when (eq? 'failure (test)) (error 'guile \"fail!\"))"

.PHONY : all check-chez check-guile
