(define (parse-test test)
  (let ((input (lookup 'input test)))
    `(lambda ()
       (test-success ,(lookup 'description test)
                     =
                     knapsack
                     '(,(lookup 'maximumWeight input)
                       (,@(map (lookup-partial 'weight)
                                (lookup 'items input)))
                       (,@(map (lookup-partial 'value)
                                (lookup 'items input))))
                     ,(lookup 'expected test)))))

